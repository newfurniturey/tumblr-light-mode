{
    "name": "Tumblr Light Mode",
    "version": "1.0",
    "description": "Tired of dark memes? Lighten up!",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": [
            "https://www.tumblr.com/*"
        ],
        "css": [
            "css/light-theme.css"
        ]
    }],
    "icons": {
        "128": "images/icon_128x128.png",
        "16": "images/icon_16x16.png",
        "48": "images/icon_48x48.png"
    }
}
