{
    "name": "Tumblr Light",
    "version": "1.0",
    "description": "Tired of dark memes? Lighten up!",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": [
            "https://www.tumblr.com/*"
        ],
        "css": [
            "css/light-theme.css"
        ]
    }]
}
